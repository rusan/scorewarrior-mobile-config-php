{
    "name": "scorewarriors/mobile-config",
    "description": "Mobile App Configuration Service",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "ext-phalcon": "^5.9",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "scripts/run-tests.sh"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}